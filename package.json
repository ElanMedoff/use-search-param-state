{
  "name": "use-search-param-state",
  "version": "0.1.0",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/csj/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ElanMedoff/use-search-param-state"
  },
  "bugs": {
    "url": "https://github.com/ElanMedoff/use-search-param-state/issues"
  },
  "homepage": "https://github.com/ElanMedoff/use-search-param-state#readme",
  "license": "MIT",
  "scripts": {
    "build-types": "tsc --project ./tsconfig.types.json",
    "build-cjs": "tsc --project ./tsconfig.cjs.json",
    "build-esm": "tsc --project ./tsconfig.esm.json",
    "build": "npm run build-cjs && npm run build-esm && npm run build-types",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "prettier": "prettier src/** --write --config .prettierrc",
    "lint": "eslint src/**"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^20.8.2",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "^16.14.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "vitest-dom": "^0.1.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
